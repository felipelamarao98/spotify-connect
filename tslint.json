{
    "defaultSeverity": "error",
    "extends": [
        "tslint-config-airbnb",
        "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {
        "no-trailing-whitespace":true,
        "space-before-function-paren":false,
        "max-line-length": [
            true,
            {
                "limit":120,
                "ignore-pattern":"^import |^export {(.*?)}",
            }
        ]
    },
    "rulesDirectory": [],
    "linterOptions": {
        "exclude": ["**/node_modules/**"]
    }
}